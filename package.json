{
  "name": "jalan-raya",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:krsbx/jalan-raya.git",
  "author": "krsbx <firdaus.sati51@gmail.cm>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/index.js",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "prepare": "husky install && patch-package && sequelize-db-type",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:create": "sequelize-cli seed:create",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "pretest": "yarn build",
    "test": "cross-env IN_TEST=true jest -i ./dist/test/*.test.js --detectOpenHandles"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@krsbx/sequelize-fqp": "^0.1.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sequelize-cli": "^6.6.0",
    "sequelize-db-type": "^0.3.7",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  },
  "dependencies": {
    "@krsbx/response-formatter": "^1.0.0",
    "app-root-path": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-asyncmw": "^0.2.5",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0"
  },
  "sequelize-db-type": {
    "models": "src/models",
    "migrations": "src/migrations",
    "seeders": "src/seeders"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
